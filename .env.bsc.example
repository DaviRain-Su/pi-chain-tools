# BSC live execution (copy to .env.bsc.local and fill real values)

# Required for live execution
BSC_EXECUTE_MODE=native
BSC_EXECUTE_PRIVATE_KEY=0xYOUR_PRIVATE_KEY
BSC_EXECUTE_RECIPIENT=0xYOUR_BSC_WALLET_ADDRESS

# Recommended RPC
BSC_RPC_URL=https://bsc-dataseed.binance.org

# Safety defaults (adjust as needed)
BSC_EXECUTE_CONFIRMATIONS=1
BSC_YIELD_MIN_APR_DELTA_BPS=30
BSC_YIELD_REBALANCE_INTERVAL_DAYS=7
BSC_YIELD_DEFAULT_QUOTE_USD=100

# Protocol preference
BSC_YIELD_EXECUTION_PROTOCOL_DEFAULT=venus

# Autonomous track template (default off; keeps legacy behavior when false)
BSC_AUTONOMOUS_MODE=false

# AsterDEX execute-binding seam (autonomous track, additive + guarded)
BSC_AUTONOMOUS_ASTERDEX_ENABLED=false
BSC_AUTONOMOUS_ASTERDEX_API_BASE_URL=https://api.asterdex.com
BSC_AUTONOMOUS_ASTERDEX_TIMEOUT_MS=3000
BSC_AUTONOMOUS_ASTERDEX_EXECUTE_BINDING_ENABLED=false
BSC_AUTONOMOUS_ASTERDEX_EXECUTE_BINDING_REQUIRED=false
BSC_AUTONOMOUS_ASTERDEX_EXECUTE_ACTIVE=false
BSC_AUTONOMOUS_ASTERDEX_EXECUTE_COMMAND=node scripts/asterdex-exec-safe.mjs "{intent}"
BSC_AUTONOMOUS_ASTERDEX_ROUTER_ADDRESS=
BSC_AUTONOMOUS_ASTERDEX_EXECUTOR_ADDRESS=
BSC_AUTONOMOUS_ASTERDEX_LIVE_COMMAND=node contracts/bsc-autonomous/scripts/run-cycle.mjs --contract ${BSC_AUTONOMOUS_CONTRACT_ADDRESS}
BSC_AUTONOMOUS_ASTERDEX_CONFIRM_TEXT=ASTERDEX_EXECUTE_LIVE
BSC_AUTONOMOUS_ASTERDEX_MAX_AMOUNT_RAW=1000000000000000000
BSC_AUTONOMOUS_CONTRACT_ENTRYPOINT_ENABLED=true
BSC_AUTONOMOUS_CONTRACT_ADDRESS=
BSC_AUTONOMOUS_ROUTER_ADDRESS=
BSC_AUTONOMOUS_CONTRACT_MAX_AMOUNT_RAW=1000000000000000000
BSC_AUTONOMOUS_CONTRACT_COOLDOWN_SECONDS=300
BSC_AUTONOMOUS_EMERGENCY_ADMIN=
BSC_AUTONOMOUS_ASTERDEX_TOKEN_IN_ADDRESS=
BSC_AUTONOMOUS_ASTERDEX_TOKEN_OUT_ADDRESS=

# Testnet/mainnet deployment placeholders
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545
BSC_TESTNET_PRIVATE_KEY=0xYOUR_TESTNET_PRIVATE_KEY
BSC_MAINNET_RPC_URL=https://bsc-dataseed.binance.org
BSC_MAINNET_PRIVATE_KEY=0xYOUR_MAINNET_PRIVATE_KEY
BSC_SCAN_API_KEY=

# Optional protocol-specific signer keys (fallback to BSC_EXECUTE_PRIVATE_KEY if empty)
BSC_LISTA_EXECUTE_PRIVATE_KEY=
BSC_WOMBAT_EXECUTE_PRIVATE_KEY=
BSC_AAVE_EXECUTE_PRIVATE_KEY=

# SDK switches (optional)
BSC_VENUS_USE_SDK=true
BSC_LISTA_USE_SDK=true
BSC_WOMBAT_USE_SDK=true
