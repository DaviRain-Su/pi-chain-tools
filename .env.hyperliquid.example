# BSC live execution (copy to .env.bsc.local and fill real values)

# Required for live execution
BSC_EXECUTE_MODE=native
BSC_EXECUTE_PRIVATE_KEY=0xYOUR_PRIVATE_KEY
BSC_EXECUTE_RECIPIENT=0xYOUR_BSC_WALLET_ADDRESS

# Recommended RPC
BSC_RPC_URL=https://bsc-dataseed.binance.org

# Safety defaults (adjust as needed)
BSC_EXECUTE_CONFIRMATIONS=1
BSC_YIELD_MIN_APR_DELTA_BPS=30
BSC_YIELD_REBALANCE_INTERVAL_DAYS=7
BSC_YIELD_DEFAULT_QUOTE_USD=100

# Protocol preference
BSC_YIELD_EXECUTION_PROTOCOL_DEFAULT=venus

# Autonomous track template (default off; keeps legacy behavior when false)
HYPERLIQUID_AUTONOMOUS_MODE=false

# Hyperliquid execute-binding seam (autonomous track, additive + guarded)
HYPERLIQUID_AUTONOMOUS_ENABLED=false
HYPERLIQUID_AUTONOMOUS_API_BASE_URL=https://api.hyperliquid.com
HYPERLIQUID_AUTONOMOUS_TIMEOUT_MS=3000
HYPERLIQUID_AUTONOMOUS_EXECUTE_BINDING_ENABLED=false
HYPERLIQUID_AUTONOMOUS_EXECUTE_BINDING_REQUIRED=false
HYPERLIQUID_AUTONOMOUS_EXECUTE_ACTIVE=false
HYPERLIQUID_AUTONOMOUS_EXECUTE_COMMAND=node scripts/hyperliquid-exec-safe.mjs "{intent}"
HYPERLIQUID_AUTONOMOUS_ROUTER_ADDRESS=
HYPERLIQUID_AUTONOMOUS_EXECUTOR_ADDRESS=
HYPERLIQUID_AUTONOMOUS_LIVE_COMMAND=node contracts/hyperliquid-autonomous/scripts/run-cycle.mjs --contract ${HYPERLIQUID_AUTONOMOUS_CONTRACT_ADDRESS}
HYPERLIQUID_AUTONOMOUS_CONFIRM_TEXT=HYPERLIQUID_EXECUTE_LIVE
HYPERLIQUID_AUTONOMOUS_MAX_AMOUNT_RAW=1000000000000000000
HYPERLIQUID_AUTONOMOUS_CONTRACT_ENTRYPOINT_ENABLED=true
HYPERLIQUID_AUTONOMOUS_CONTRACT_ADDRESS=
HYPERLIQUID_AUTONOMOUS_ROUTER_ADDRESS=
HYPERLIQUID_AUTONOMOUS_CONTRACT_MAX_AMOUNT_RAW=1000000000000000000
HYPERLIQUID_AUTONOMOUS_CONTRACT_COOLDOWN_SECONDS=300
HYPERLIQUID_AUTONOMOUS_EMERGENCY_ADMIN=
HYPERLIQUID_AUTONOMOUS_TOKEN_IN_ADDRESS=
HYPERLIQUID_AUTONOMOUS_TOKEN_OUT_ADDRESS=

# Testnet/mainnet deployment placeholders
HYPERLIQUID_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545
HYPERLIQUID_TESTNET_PRIVATE_KEY=0xYOUR_TESTNET_PRIVATE_KEY
BSC_MAINNET_RPC_URL=https://bsc-dataseed.binance.org
BSC_MAINNET_PRIVATE_KEY=0xYOUR_MAINNET_PRIVATE_KEY
BSC_SCAN_API_KEY=

# Optional protocol-specific signer keys (fallback to BSC_EXECUTE_PRIVATE_KEY if empty)
BSC_LISTA_EXECUTE_PRIVATE_KEY=
BSC_WOMBAT_EXECUTE_PRIVATE_KEY=
BSC_AAVE_EXECUTE_PRIVATE_KEY=

# SDK switches (optional)
BSC_VENUS_USE_SDK=true
BSC_LISTA_USE_SDK=true
BSC_WOMBAT_USE_SDK=true
